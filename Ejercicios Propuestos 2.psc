Algoritmo area_volumen
	Escribir "Ingresar numero 1";
	Leer numero1;
	Escribir "Ingrese numero 2";
	Leer numero2;
	sumaNumeros <- numero1+numero2;
	MultiplicacionNumeros <- numero1*numero2;
	Escribir "La suma de los numeros es", sumaNumeros;
	Escribir "La multiplicacion de los numeros es", multiplicacionNumeros;
FinAlgoritmo
